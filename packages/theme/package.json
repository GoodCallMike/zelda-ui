{
  "name": "zelda-ui-theme",
  "version": "0.1.0-alpha.11",
  "description": "Theme system and design tokens for the Zelda UI library",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./css": "./dist/zelda-ui-theme.css"
  },
  "style": "dist/zelda-ui-theme.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/dynamic": "^2.1.5",
    "@vanilla-extract/recipes": "^0.5.7",
    "@vanilla-extract/vite-plugin": "^4.0.20",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.3.0"
  },
  "keywords": [
    "theme",
    "design-tokens",
    "css",
    "vanilla-extract",
    "zelda"
  ],
  "publishConfig": {
    "access": "public"
  }
}
